{
  "name": "laracasts-chrome-extension",
  "version": "0.1.0",
  "authors": [
    "Mario Basic <mario.basic@outlook.com>"
  ],
  "description": "This extension notifies you of new lessons, series and lets you view the latest.",
  "keywords": [
    "laracasts",
    "notifier",
    "chrome",
    "extension"
  ],
  "license": "MIT",
  "private": true,
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "bootstrap": "~3.3.1",
    "react": "~0.12.1",
    "phpjs": "*"
  },
  "install": {
    "path": "libs",
    "sources": {
      "phpjs": [
        "bower_components/phpjs/functions/array/array_diff.js",
        "bower_components/phpjs/functions/array/count.js",
        "bower_components/phpjs/functions/json/json_encode.js",
        "bower_components/phpjs/functions/json/json_decode.js"
      ]
    }
  }
}
