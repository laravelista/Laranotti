{
  "manifest_version": 2,
  "name": "Laracasts Notifier",
  "description": "This extension notifies you of new lessons, series and lets you view the latest.",
  "version": "0.3.0",
  "permissions": [
    "http://laracasts-feed.mariobasic.app/api/v1/feed/lessons",
    "notifications",
    "storage",
    "alarms",
    "tabs"
  ],
  "background": {
    "scripts": [
      "public/js/vendor/jquery.js",
      "public/js/helpers.js",
      "public/js/eventPage.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "laracasts.png",
    "default_popup": "popup.html"
  }
}