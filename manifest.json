{
  "manifest_version": 2,
  "name": "Laracasts Notifier v2",
  "description": "This extension notifies you of new lessons from Laracasts, and lets you view the latest.",
  "version": "2.0.0",
  "icons": {
    "16": "laracasts16.png",
    "48": "laracasts48.png",
    "128": "laracasts128.png"
  },
  "permissions": [
    "http://laracasts-feed.mariobasic.com/api/v1/feed/lessons",
    "notifications",
    "storage",
    "alarms",
    "tabs"
  ],
  "background": {
    "scripts": [
      "public/js/vendor/jquery.js",
      "public/js/helpers.js",
      "public/js/eventPage.js"
    ],
    "persistent": false
  },
  "browser_action": {
    "default_icon": "laracasts16.png",
    "default_popup": "popup.html"
  }
}