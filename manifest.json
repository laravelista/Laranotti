{
  "manifest_version": 2,
  "name": "Laracasts Notifier",
  "description": "This extension notifies you of new lessons, series and lets you view the latest.",
  "version": "0.1.0",
  "permissions": [
    "http://laracasts-feed.mariobasic.app/api/v1/feed/lessons",
    "notifications",
    "storage"
  ],
  "browser_action": {
    "default_icon": "laracasts.png",
    "default_popup": "popup.html"
  }
}